{
    "manifest_version": 3,
    "name": "Ad Blocker",
    "version": "1.0",
    "description": "Youtube reklam engelleyicisi",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  